<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>SpecFlow.xUnit</id>
    <version>$version$</version>
    <title>SpecFlow.xUnit</title>
    <authors>TechTalk</authors>
    <owners>TechTalk</owners>
    <description>Package to use SpecFlow with xUnit 2.4 and later</description>
    <summary>Package to use SpecFlow with xUnit 2.4 and later</summary>
    <language>en-US</language>
    <projectUrl>http://www.specflow.org</projectUrl>
    <iconUrl>http://go.specflow.org/specflow-nuget-icon</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="file">LICENSE.txt</license>
    <tags>specflow xUnit</tags>
    <dependencies>
      <dependency id="SpecFlow" version="[$version$]" />
      <dependency id="xunit.core" version="2.4.0" />
      <dependency id="Xunit.SkippableFact" version="1.3.12" />
    </dependencies>
  </metadata>

  <files>
    <file src="build\**\*" target="build" />
    <file src="bin\$config$\net471\TechTalk.SpecFlow.xUnit.SpecFlowPlugin.*" target="lib\$SpecFlow_FullFramework_Runtime_TFM$" />
    <file src="bin\$config$\netstandard2.0\TechTalk.SpecFlow.xUnit.SpecFlowPlugin.dll" target="lib\$SpecFlow_Core_Runtime_TFM$" />
    <file src="bin\$config$\netstandard2.0\TechTalk.SpecFlow.xUnit.SpecFlowPlugin.pdb" target="lib\$SpecFlow_Core_Runtime_TFM$" />

    <file src="bin\$config$\net471\TechTalk.SpecFlow.xUnit.Generator.SpecFlowPlugin.*" target="build\$SpecFlow_FullFramework_Generator_TFM$" />
    <file src="bin\$config$\netstandard2.0\TechTalk.SpecFlow.xUnit.Generator.SpecFlowPlugin.dll" target="build\$SpecFlow_Core_Generator_TFM$" />
    <file src="bin\$config$\netstandard2.0\TechTalk.SpecFlow.xUnit.Generator.SpecFlowPlugin.pdb" target="build\$SpecFlow_Core_Generator_TFM$" />

    <file src="$SolutionDir$LICENSE.txt" target="LICENSE.txt" />
  </files>
</package>
