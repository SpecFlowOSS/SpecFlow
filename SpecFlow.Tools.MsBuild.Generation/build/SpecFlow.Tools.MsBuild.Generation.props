<Project TreatAsLocalProperty="TaskFolder;TaskAssembly">

  <PropertyGroup>
    
    <_SpecFlow_TaskFolder Condition=" '$(MSBuildRuntimeType)' == 'Core' And '$(_SpecFlow_TaskFolder)' == ''">netstandard1.6</_SpecFlow_TaskFolder>
    <_SpecFlow_TaskFolder Condition=" '$(MSBuildRuntimeType)' != 'Core' And '$(_SpecFlow_TaskFolder)' == ''">net461</_SpecFlow_TaskFolder>
    <_SpecFlow_TaskAssembly Condition=" '$(_SpecFlow_TaskAssembly)' == '' "> $(MSBuildThisFileDirectory)..\tasks\$(_SpecFlow_TaskFolder)\TechTalk.SpecFlow.MSBuildTasks.dll</_SpecFlow_TaskAssembly>
  </PropertyGroup>

</Project>